{
    "dataset_reader": {
        "question_token_indexers": {
            "tokens": {
                "lowercase_tokens": true,
                "type": "single_id"
            },
            "elmo": {
                "type": "elmo_characters"
            }
        },
        "type": "friction_q",
        "sample": -1,
        "filter_type2": false,
        "lazy": false,
        "lf_syntax": "with_type_2_general_attr_entities",
        "replace_world_entities": true,
        "single_lf_extractor_aligned": true,
        "skip_attributes_regex": "loudness",
        "lexical_cues": [
            "synonyms",
            "values"
        ]
    },
    "validation_data_path": "/inputs/generalqr-v1-taglbwe/generalqr-v1-taglbwe-dev.jsonl",
    "train_data_path": "/inputs/generalqr-v1-taglbwe/generalqr-v1-taglbwe-train.jsonl",
    "validation_dataset_reader": {
        "question_token_indexers": {
            "tokens": {
                "lowercase_tokens": true,
                "type": "single_id"
            },
            "elmo": {
                "type": "elmo_characters"
            }
        },
        "type": "friction_q",
        "sample": -1,
        "filter_type2": false,
        "lazy": false,
        "lf_syntax": "with_type_2_general_attr_entities",
        "replace_world_entities": true,
        "skip_world_alignment": true,
        "skip_attributes_regex": "loudness",
        "lexical_cues": [
            "synonyms",
            "values"
        ]
    },
    "model": {
        "num_linking_features": 0,
        "type": "friction_q_parser",
        "attention_function": {
            "type": "dot_product"
        },
        "use_entities": true,
        "max_decoding_steps": 30,
        "question_embedder": {
            "tokens": {
                "num_embeddings": 200000,
                "type": "embedding",
                "pretrained_file": "/glove/glove.6B.100d.txt.gz",
                "embedding_dim": 100,
                "trainable": false
            },
            "elmo": {
                "options_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_options.json",
                "type": "elmo_token_embedder",
                "weight_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_weights.hdf5",
                "dropout": 0.5,
                "do_layer_norm": false
            }
        },
        "entity_similarity_mode": "weighted_dot_product",
        "action_embedding_dim": 50,
        "encoder": {
            "type": "lstm",
            "input_size": 1124,
            "hidden_size": 50,
            "num_layers": 1,
            "bidirectional": false
        },
        "decoder_beam_search": {
            "beam_size": 5
        },
        "dropout": 0.2
    },
    "vocabulary": {
        "non_padded_namespaces": [
            "actions"
        ]
    },
    "iterator": {
        "type": "basic",
        "batch_size": 5
    },
    "trainer": {
        "validation_metric": "+parse_acc",
        "num_epochs": 100,
        "cuda_device": 0,
        "patience": 50,
        "learning_rate_scheduler": {
            "mode": "max",
            "type": "reduce_on_plateau",
            "patience": 10,
            "factor": 0.5
        },
        "num_serialized_models_to_keep": 2,
        "optimizer": {
            "type": "adam"
        }
    }
}